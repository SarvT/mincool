	"tokenColors": [
	  {
		"name": "Comments",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#5363EE99"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": {
		  "foreground": "#ffffff"
		}
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Keywords",
		"scope": ["keyword", "storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#5363EE"
		}
	  },
	  {
		"name": "Control Flow",
		"scope": ["keyword.control", "keyword.operator.new"],
		"settings": {
		  "foreground": "#5363EE"
		}
	  },
	  {
		"name": "Operators",
		"scope": ["keyword.operator"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Functions",
		"scope": ["entity.name.function", "meta.function-call", "support.function"],
		"settings": {
		  "foreground": "#3545D6"
		}
	  },
	  {
		"name": "Classes",
		"scope": ["entity.name.type", "entity.name.class", "entity.name.namespace", "entity.name.scope-resolution"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Strings",
		"scope": ["string", "punctuation.definition.string"],
		"settings": {
		  "foreground": "#C2F750CC"
		}
	  },
	  {
		"name": "Numbers",
		"scope": ["constant.numeric"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Constants",
		"scope": ["constant.language", "support.constant", "constant.character", "constant.other"],
		"settings": {
		  "foreground": "#5363EE"
		}
	  },
	  {
		"name": "Tags",
		"scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
		"settings": {
		  "foreground": "#5363EE"
		}
	  },
	  {
		"name": "Attributes",
		"scope": ["entity.other.attribute-name"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Property names",
		"scope": ["support.type.property-name"],
		"settings": {
		  "foreground": "#3545D6"
		}
	  },
	  {
		"name": "Property values",
		"scope": ["support.constant.property-value"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Escape Characters",
		"scope": ["constant.character.escape"],
		"settings": {
		  "foreground": "#C2F750"
		}
	  },
	  {
		"name": "Decorators",
		"scope": ["meta.decorator", "tag.decorator.js entity.name.tag", "tag.decorator.js punctuation.definition.tag"],
		"settings": {
		  "foreground": "#3545D6",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#FF5555"
		}
	  },
	  {
		"name": "Markdown Headings",
		"scope": ["markup.heading"],
		"settings": {
		  "foreground": "#C2F750",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Markdown Links",
		"scope": ["markup.underline.link", "string.other.link"],
		"settings": {
		  "foreground": "#5363EE"
		}
	  },
	  {
		"name": "Markdown Bold",
		"scope": ["markup.bold"],
		"settings": {
		  "foreground": "#ffffff",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Markdown Italic",
		"scope": ["markup.italic"],
		"settings": {
		  "foreground": "#ffffff",
		  "fontStyle": "italic"
		}
	  }
	]
  }